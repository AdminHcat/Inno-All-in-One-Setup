// Prototypes.
function LoadIcon(iconType : TMsgBoxType; bitmap : TBitmap) : TColor; forward;

//*****************************************************************************
//
//  Routine: LoadIcon
//
//  Description: Load an bitmap icon.
//
//  Calling Sequence:
//      ReplaceColor := LoadIcon(iconType, bimtap);
//
//  Parameters:
//      iconType - The type of icon to load.
//      bitmap   - Where to load the icon.
//
//  Routines Called:
//
//  Global Variables Referenced:
//
//  Notes:
//      iconType = (mbInformation, mbConfirmation, mbError, mbCriticalError).
//
//  Return Values:
//      TColor - The icon transperancy color.
//
//*****************************************************************************

function LoadIcon(iconType : TMsgBoxType; bitmap : TBitmap) : TColor;
var
    iconBitmap   : String;
    iconFilename : String;

begin
    // The icons will be loaded from the tmp directory.
    iconFilename := ExpandConstant('{tmp}');

    // Determine which icon to load into the bitmap region.
    case iconType of
        mbConfirmation :
            begin
                // Pick the confirmation bitmap.
                iconBitmap   := CONFIRMATION_ICON_BITMAP;
                iconFilename := AddBackSlash(iconFilename) + CONFIRMATION_ICON_FILENAME;
            end;

        mbCriticalError :
            begin
                // Pick the critical error bitmap.
                iconBitmap   := CRITICAL_ERROR_ICON_BITMAP;
                iconFilename := AddBackSlash(iconFilename) + CRITICAL_ERROR_ICON_FILENAME;
            end;

        mbError :
            begin
                // Pick the error bitmap.
                iconBitmap   := ERROR_ICON_BITMAP;
                iconFilename := AddBackSlash(iconFilename) + ERROR_ICON_FILENAME;
            end;

        mbInformation :
            begin
                // Pick the information bitmap.
                iconBitmap   := INFORMATION_ICON_BITMAP;
                iconFilename := AddBackSlash(iconFilename) + INFORMATION_ICON_FILENAME;
            end;
    end;

    if (not FileExists(iconFilename))
    then begin
        // The icon needs to be created before it can be loaded.
        SaveStringToFile(iconFilename, iconBitmap, False);
    end;

    // Load the icon into the itmap region.
    bitmap.LoadFromFile(iconFilename);

    // Return the transparency color.
    Result := TRANSPERANCY_COLOR;
end;
